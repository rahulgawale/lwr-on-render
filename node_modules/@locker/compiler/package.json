{
    "name": "@locker/compiler",
    "version": "0.17.9",
    "license": "SEE LICENSE IN LICENSE.txt",
    "author": "Salesforce UI Security Team",
    "description": "Lightning Web Security module compiler",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup --config .rolluprc.cjs",
        "build:dev": "cross-env NODE_ENV=development yarn build",
        "clean": "locker-trash dist/",
        "test:base": "yarn build && jest",
        "test:dev": "cross-env NODE_ENV=development yarn test:base",
        "test:update": "yarn test:dev --updateSnapshot --testPathIgnorePatterns=\"utils.spec.js\" && yarn test:base --updateSnapshot --testPathIgnorePatterns=\"utils.spec.js\"",
        "test": "yarn test:dev && yarn test:base"
    },
    "dependencies": {
        "v8-compile-cache": "2.3.0"
    },
    "devDependencies": {
        "@jridgewell/sourcemap-codec": "1.4.14",
        "@locker/ast-lib-maker": "0.17.9",
        "@locker/shared": "0.17.9",
        "acorn": "8.8.0",
        "acorn-walk": "8.2.0",
        "magic-string": "0.26.3",
        "merge-options": "3.0.4",
        "meriyah": "4.3.1",
        "normalize-path": "3.0.0"
    },
    "files": [
        "dist/",
        "docs/",
        "LICENSE.txt"
    ],
    "gitHead": "30d845dad4cbf8ea85c76f3a7c761bbd5453f43a"
}
