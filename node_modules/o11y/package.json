{
    "name": "o11y",
    "version": "242.3.3",
    "description": "O11y Client-Side Instrumentation & Observability Library",
    "main": "index.js",
    "exports": {
        ".": "./index.js",
        "./client": "./dist/modules/o11y/client/client.js",
        "./collectors": "./dist/modules/o11y/collectors/collectors.js",
        "./core_envelope": "./dist/modules/o11y/core_envelope/core_envelope.js",
        "./simple_collector": "./dist/modules/o11y/simple_collector/simple_collector.js",
        "./shared": "./dist/modules/o11y/shared/shared.js",
        "./web_vitals": "./dist/modules/o11y/web_vitals/web_vitals.js"
    },
    "scripts": {
        "clean": "rimraf dep-cache*.tgz dist && jest --clearCache",
        "build": "rollup -c ./rollup.config.js",
        "build:clean": "yarn clean && yarn build",
        "test": "yarn --cwd ../.. jest --config ./jest.config.js --selectProjects 'o11y-client'",
        "test:clean": "yarn clean && yarn test",
        "test:debug": "NODE_OPTIONS='--inspect-brk'; yarn test --runInBand",
        "test:integration": "TEST_FILTER=int yarn test --runInBand --bail",
        "prettier-write": "prettier --config .prettierrc.json '**/*.ts' '**/*.js' '**/*.json' --write",
        "eslint": "eslint './**/*.ts' './**/*.js' --fix"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=12",
        "yarn": "^1.22"
    },
    "volta": {
        "node": "15.10.0",
        "yarn": "1.22.10"
    },
    "repository": {
        "type": "git",
        "url": "git@git.soma.salesforce.com:instrumentation/o11y-client.git"
    },
    "author": "Instrumentation Team",
    "license": "BSD-3-Clause",
    "dependencies": {
        "o11y_schema": "242.2.0",
        "protobufjs": "6.11.3",
        "web-vitals": "2.1.4"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^6.0.0",
        "@types/jest": "^26.0.14",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-jest": "^24.2.0",
        "husky": "^4",
        "jest": "^26.4.2",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^10.5.4",
        "open-cli": "^6.0.1",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.32.0",
        "rollup-plugin-external-globals": "^0.6.1",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^26.5.0",
        "tslib": "^2.0.3",
        "typescript": "^4.5.4",
        "xhr-mock": "^2.5.1"
    },
    "files": [
        "index.js",
        "client.d.ts",
        "dist/modules/o11y/**"
    ],
    "lwc": {
        "modules": [
            {
                "dir": "dist/modules"
            }
        ],
        "expose": [
            "o11y/client",
            "o11y/collectors",
            "o11y/core_envelope",
            "o11y/shared",
            "o11y/simple_collector",
            "o11y/web_vitals"
        ]
    },
    "type": "module"
}
