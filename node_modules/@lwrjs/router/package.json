{
    "name": "@lwrjs/router",
    "description": "LWR Client-side Navigation APIs and services",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.8.0",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce/lwr.git",
        "directory": "packages/@lwrjs/router"
    },
    "bugs": {
        "url": "https://github.com/salesforce/lwr/issues"
    },
    "type": "module",
    "types": "build/services/index.d.ts",
    "main": "build/cjs/services/index.cjs",
    "module": "build/services/index.js",
    "exports": {
        ".": {
            "import": "./build/services/index.js",
            "require": "./build/cjs/services/index.cjs"
        },
        "./module-provider": {
            "import": "./build/services/module-provider/index.js",
            "require": "./build/cjs/services/module-provider/index.cjs"
        },
        "./pageObjects/*": {
            "import": "./pageObjects/*.js",
            "require": "./pageObjects/*.cjs"
        }
    },
    "files": [
        "build/**/*.js",
        "build/**/*.cjs",
        "build/**/*.d.ts",
        "build/**/*.js-meta.xml",
        "build/**/*.css",
        "build/**/*.html",
        "pageObjects"
    ],
    "scripts": {
        "build": "node ../../../bin/pack-lwc --dir src/modules build/modules && yarn utam:compile && yarn build:bundle",
        "build:bundle": "rollup --config scripts/rollup.moduleBundle.config.cjs",
        "utam:compile": "utam -c utam.config.cjs -t module"
    },
    "dependencies": {
        "@lwrjs/client-modules": "0.8.0",
        "@lwrjs/diagnostics": "0.8.0",
        "@lwrjs/shared-utils": "0.8.0",
        "ajv": "6.12.6"
    },
    "lwc": {
        "modules": [
            {
                "dir": "build/modules"
            },
            {
                "name": "lightning/navigation",
                "path": "build/modules/lwr/navigation/navigation.js"
            }
        ],
        "expose": [
            "lwr/routerContainer",
            "lwr/navigation",
            "lightning/navigation",
            "lwr/outlet",
            "lwr/router",
            "lwr/routerErrors",
            "lwr/routerBridge"
        ]
    },
    "engines": {
        "node": ">=14.15.4 <19"
    },
    "gitHead": "6db460e470fb149ca282aaa293ad4f0be03ae4e8"
}
